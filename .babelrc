{
  "env": {
    "node": {
      "presets": [[ "@babel/env", { "targets": { "node": 6  } } ]],
      "plugins": [
        "add-module-exports",
        "./babel/inject"
      ]
    },
    "cjs": {
      "presets": [[ "@babel/env", { "modules": "commonjs", "shippedProposals": true } ]],
      "plugins": [
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [[ "@babel/env", { "modules": false, "shippedProposals": true } ]]
    }
  }
}
