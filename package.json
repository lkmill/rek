{
  "name": "rek",
  "version": "0.5.3",
  "description": "Wrapper around the Fetch API",
  "main": "./cjs/browser.js",
  "module": "./esm/browser.mjs",
  "source": "./src/browser.mjs",
  "types": "./types.d.ts",
  "exports": {
    ".": "./cjs/browser.js",
    "./node": "./cjs/node.js"
  },
  "scripts": {
    "clean": "rimraf cjs dist esm",
    "format": "prettier --write .",
    "lint": "eslint src --ext mjs",
    "build": "npm run build:cjs && npm run build:esm && npm run build:dist",
    "build:cjs": "BABEL_ENV=cjs babel src -s --out-dir cjs",
    "build:esm": "BABEL_ENV=esm babel src -s --keep-file-extension --out-dir esm",
    "build:dist": "BABEL_ENV=esm rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "preversion": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lohfu/rek.git"
  },
  "keywords": [
    "ajax",
    "curl",
    "fetch",
    "fetching",
    "http",
    "https",
    "node-fetch",
    "request",
    "requests",
    "wget",
    "xhr"
  ],
  "author": "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lohfu/rek/issues"
  },
  "homepage": "https://github.com/lohfu/rek#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.3.1",
    "rollup": "^2.51.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4"
  }
}
